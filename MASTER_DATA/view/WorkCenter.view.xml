											<Input value="{WORKCENTER_DESCR}" tooltip="{WORKCENTER_DESCR}" editable="false" width="100%" />
										</table:template>
									</table:Column>
									<table:Column width="13%" hAlign="Center" >
										<Label text="{i18n&gt;viewWRK.table.position}" />
