									<Label text="{ID_FASE}" tooltip="{ID_FASE}" />
								</table:template>
							</table:Column>
							<table:Column width="20%" hAlign="Begin">
								<Label text="{i18n&gt;viewUplod.AttachName}" wrapping="true"/>
								<table:template>
									<Label text="{NOME_ALLEGATO}" tooltip="{NOME_ALLEGATO}" />
								</table:template>
							</table:Column>
							<table:Column width="72%" hAlign="Begin">
								<Label text="{i18n&gt;viewUplod.Url}" wrapping="true"/>
								<table:template>
									<Label text="{URL}" tooltip="{URL}" />
								</table:template>
							</table:Column>								
						</table:columns>
					</table:Table>
				</HBox>
			</u:ObjectPageSubSection>
		</u:ObjectPageSection>
		<u:footer>
			<Toolbar style="Clear" design="Transparent">
				<!-- footer ObjectPageSection id="CHECKLIST"-->
				<HBox width="25%" height="15%" renderType="Bare">
					<HBox id="HBoxUpload" width="100%" height="100%" renderType="Bare" alignItems="Center" />							
				</HBox>
				<Button text="{i18n&gt;viewUplod.download}" tooltip="{i18n&gt;viewUplod.download}" icon="sap-icon://excel-attachment" type="Transparent" press="downloadModel"/>
				<ToolbarSpacer />
				<Button id="btnCloseUpload" enabled="false" text="{i18n&gt;viewUplod.resetBtn}" tooltip="{i18n&gt;viewUplod.resetBtn}" icon="sap-icon://undo" press="clearupload" type="Reject" /> 							
				<Button id="btnSaveUpload" enabled="false" text="{i18n&gt;viewUplod.saveBtn}" tooltip="{i18n&gt;viewUplod.saveBtn}" icon="sap-icon://save" press="uploadSave" type="Accept"/>
			</Toolbar>
		</u:footer>
	</u:ObjectPageLayout>
</mvc:View>