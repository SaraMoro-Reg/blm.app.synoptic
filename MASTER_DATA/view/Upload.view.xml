<?xml version="1.0" encoding="UTF-8"?>
<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:sample="sap.uxap.sample.ObjectPageSubSectionSized.controller.blocks" xmlns:table="sap.ui.table" xmlns:u="sap.uxap" controllerName="master_data.controller.Upload" height="100%" displayBlock="true">
	<u:ObjectPageLayout id="phaseObjectPage" enableLazyLoading="true" navigate="pressTabBar" showTitleInHeaderContent="false" upperCaseAnchorBar="false" useIconTabBar="true" backgroundDesignAnchorBar="Transparent" showHeaderContent="false" height="100%" class="sapUiNoContentPadding" showFooter="true">
		<!-- Checklist -->
		<u:ObjectPageSection id="CHECKLIST" title="{i18n&gt;viewUplod.checklist}">
			<u:ObjectPageSubSection>
				<HBox width="100%" height="90%" renderType="Bare">
					<table:Table id="tabChecklist" rows="{path: '/tabCheckList'}" width="100%" selectionMode="None" visibleRowCountMode="Interactive" >
						<table:columns>
							<table:Column width="8%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.Lang}" wrapping="true"/>
								<table:template>
									<Label text="{LINGUA}" tooltip="{LINGUA}" />
								</table:template>
							</table:Column>
							<table:Column width="10%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.kmat}" wrapping="true"/>
								<table:template>
									<Label text="{KMAT}" tooltip="{KMAT}" />
								</table:template>
							</table:Column>	
							<table:Column width="15%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.machineId}" wrapping="true"/>
								<table:template>
									<Label text="{MATRICOLA_MACCHINA}" />
								</table:template>
							</table:Column>
							<table:Column width="auto" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.Odv}" wrapping="true"/>
								<table:template>
									<Label text="{ODV}" />
								</table:template>
							</table:Column>
							<table:Column width="8%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.wkc}" wrapping="true"/>
								<table:template>
									<Label text="{PIAZZOLA_PARTENZA}" tooltip="{PIAZZOLA_PARTENZA}" />
								</table:template>
							</table:Column>							
							<table:Column width="10%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.dateShip}" wrapping="true"/>
								<table:template>
									<Label text="{DATA_SPEDIZIONE}" tooltip="{DATA_SPEDIZIONE}" />
								</table:template>
							</table:Column>
							<table:Column width="10%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.NotWorkingDate}" wrapping="true"/>
								<table:template>
									<Label text="{DATE_NON_LAVORATIVE}" />
								</table:template>
							</table:Column>
							<table:Column width="8%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.CorrectionFactor}" wrapping="true"/>
								<table:template>
									<Label text="{FATTORE_CORREZIONE}" />
								</table:template>
							</table:Column>	
							<table:Column width="8%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.DocName}" wrapping="true"/>
								<table:template>
									<Label text="{NOME_DOC}" />
								</table:template>
							</table:Column>	
							<table:Column width="8%" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.DocRevision}" wrapping="true"/>
								<table:template>
									<Label text="{REV_DOC}" />
								</table:template>
							</table:Column>									
						</table:columns>
					</table:Table>
				</HBox>
			</u:ObjectPageSubSection>
		</u:ObjectPageSection>	
		<!-- Dettaglio Fase -->
		<u:ObjectPageSection id="PHASE_DETAILS" title="{i18n&gt;viewUplod.faseDetail}">
			<u:ObjectPageSubSection>
				<HBox width="100%" height="90%" renderType="Bare">
					<table:Table id="tabFaseDetails" rows="{path: '/tabFaseDetails'}" width="100%" selectionMode="None" visibleRowCountMode="Interactive" >
						<table:columns>
							<table:Column width="6.5rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.sequence}" wrapping="true" />
								<table:template>
									<Label text="{SEQUENZA}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.fase}" wrapping="true"/>
								<table:template>
									<Label text="{FASE}" />
								</table:template>
							</table:Column>
							<table:Column width="25rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.faseDescr}" wrapping="true"/>
								<table:template>
									<ExpandableText overflowMode="Popover" text="{DESCRIZIONE_FASE}" maxCharacters="40" />
								</table:template>
							</table:Column>
							<table:Column width="6rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.faseCod}" wrapping="true"/>
