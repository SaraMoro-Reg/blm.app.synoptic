									<Label text="{MACROFASE}" />
								</table:template>
							</table:Column>
							<table:Column width="6rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.paralel}" wrapping="true"/>
								<table:template>
									<Label text="{PARALLELO}" />
								</table:template>
							</table:Column>
							<table:Column width="6rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.faseType}" wrapping="true"/>
								<table:template>
									<Label text="{TIPO_FASE}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.codList}" wrapping="true"/>
								<table:template>
									<Label text="{CODICE_LISTINO}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.stdTime}" wrapping="true"/>
								<table:template>
									<Label text="{STD_TIME}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.CorrectionFactor}" wrapping="true"/>
								<table:template>
									<Label text="{FATTORE_CORREZIONE}" />
								</table:template>
							</table:Column>	
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.TimeRil}" wrapping="true"/>
								<table:template>
									<Label text="{RILEVANTE_PER_TEMPI}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.StdOp}" wrapping="true"/>
								<table:template>
									<Label text="{STD_OPERATORS}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.DcType}" wrapping="true"/>
								<table:template>
									<Label text="{TIPO_RACCOLTA_DATI}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.Param}" wrapping="true"/>
								<table:template>
									<Label text="{PARAMETRO}" />
								</table:template>
							</table:Column>
							<table:Column width="25rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.paramDesc}" wrapping="true"/>
								<table:template>
									<ExpandableText overflowMode="Popover" text="{DESCRIZIONE_PARAMETRO}" maxCharacters="40" />
								</table:template>
							</table:Column>
							<table:Column width="6rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.Um}" wrapping="true"/>
								<table:template>
									<Label text="{UM}" />
								</table:template>
							</table:Column>
							<table:Column width="7rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.Min}" wrapping="true"/>
								<table:template>
									<Label text="{MINIMO}" />
								</table:template>
							</table:Column>
							<table:Column width="7rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.Max}" wrapping="true"/>
								<table:template>
									<Label text="{MASSIMO}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.StdVal}" wrapping="true"/>
								<table:template>
									<Label text="{VALORE_STANDARD}" />
								</table:template>
							</table:Column>
							<table:Column width="7rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.OrdVisPar}" wrapping="true"/>
								<table:template>
									<Label text="{ORDINAMENTO_PARAMETRI}" />
								</table:template>
							</table:Column>
							<table:Column width="8rem" hAlign="Center">
								<Label text="{i18n&gt;viewUplod.raggrParam}" wrapping="true"/>
								<table:template>
									<ExpandableText overflowMode="Popover" maxCharacters="40" />
								</table:template>
							</table:Column>
						</table:columns>
					</table:Table>
				</HBox>
			</u:ObjectPageSubSection>
		</u:ObjectPageSection>	
		<!-- Allegati Checklist -->
		<u:ObjectPageSection id="CHECKLIST_ATT" title="{i18n&gt;viewUplod.ChecklistAttachment}">
			<u:ObjectPageSubSection>
				<HBox width="100%" height="90%" renderType="Bare">
					<table:Table id="tabChecklistAttachment" rows="{path: '/tabChecklistAttachment'}" width="100%" selectionMode="None" visibleRowCountMode="Interactive" >
						<table:columns>
							<table:Column width="20%" hAlign="Begin">
								<Label text="{i18n&gt;viewUplod.AttachName}" wrapping="true"/>
								<table:template>
									<Label text="{NOME_ALLEGATO}" tooltip="{NOME_ALLEGATO}" />
								</table:template>
							</table:Column>
							<table:Column width="80%" hAlign="Begin">
								<Label text="{i18n&gt;viewUplod.Url}" wrapping="true"/>
								<table:template>
									<Label text="{URL}" tooltip="{URL}" />
								</table:template>
							</table:Column>								
						</table:columns>
					</table:Table>
				</HBox>
			</u:ObjectPageSubSection>
		</u:ObjectPageSection>
		<!-- Allegati Fase -->
		<u:ObjectPageSection id="PHASE_ATT" title="{i18n&gt;viewUplod.PhaseAttachment}">
			<u:ObjectPageSubSection>
				<HBox width="100%" height="90%" renderType="Bare">
					<table:Table id="tabPhaseAttachment" rows="{path: '/tabPhaseAttachment'}" width="100%" selectionMode="None" visibleRowCountMode="Interactive" >
