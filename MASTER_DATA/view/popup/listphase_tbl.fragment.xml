<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
	         contentWidth="65rem"
		title="{i18n&gt;viewPhase.phaselistemptyPop}" 
		liveChange=".handleSearch"
		confirm="handleValueHelpClose" 
		cancel="handleValueHelpClose" 		
		items="{    path : '/phaselist',    
				sorter : {     path : 'SEQUENCE',     
						descending : false    }   
			}">
		<ColumnListItem selected="">
			<cells>
			<core:Icon
				src="sap-icon://{ICON}"
				class="size1"
				color="{COLORDFT}" >
				<core:layoutData>
					<FlexItemData growFactor="1" />
				</core:layoutData>
			</core:Icon>
				<ObjectIdentifier title="{SEQUENCE}" text="{OPERATIONDESCR}" />
			          <Text text="{MAINSEQUENCE}" />
			<core:Icon
				src="{PARICON}"
				class="size1"
				color="{PARCOLOR}" >
				<core:layoutData>
					<FlexItemData growFactor="1" />
				</core:layoutData>
			</core:Icon>
				<Text text="{VERSION}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="10%">
				<header>
					<Text text="Defaut" />
				</header>
			</Column>
			<Column width="50%">
				<header>
					<Text text="{i18n&gt;viewPhase.sequence}/{i18n&gt;viewPhase.phaseDescr}" />
				</header>
			</Column>
			<Column width="10%">
				<header>
					<Text text="{i18n&gt;viewPhase.macrophase}" />
				</header>
			</Column>
			<Column width="10%"
				hAlign="Center">
				<header>
					<Text text="{i18n&gt;viewPhase.phaseparal}" />
				</header>
			</Column>
			<Column width="10%"
				hAlign="Center">
				<header>
					<Text text="{i18n&gt;viewPhase.phasevers}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>