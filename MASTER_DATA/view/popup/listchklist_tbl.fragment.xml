<?xml version="1.0" encoding="UTF-8"?>
<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<TableSelectDialog noDataText="No Checklist found" title="{i18n&gt;viewAtt.selectchecklistPop}" liveChange="handleSearchChkList" confirm="handleValueHelpCloseChkList" cancel="handleValueHelpCloseChkList" items="{    path : '/checklist',    sorter : {     path : 'CHECKLIST',     descending : false    }   }">
		<ColumnListItem selected="">
			<cells>
				<Text text="{CHECKLIST}" />
				<Text text="{CHECKLIST_ID}" />
				<Text text="{STATUS}" />
				<core:Icon src="{ICON}" class="size1" color="{COLOR}">
					<core:layoutData>
						<FlexItemData growFactor="1" />
					</core:layoutData>
				</core:Icon>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="30%">
				<header>
					<Text text="{i18n&gt;viewMail.attach.nameassignment}" />
				</header>
			</Column>
			<Column visible="false">
				<header>
					<Text text="" />
				</header>
			</Column>
			<Column width="60%">
				<header>
					<Text text="" />
				</header>
			</Column>
			<Column width="10%" hAlign="Center">
				<header>
					<Text text="" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>