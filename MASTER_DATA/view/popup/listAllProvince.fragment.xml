<?xml version="1.0" encoding="UTF-8"?>
<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<TableSelectDialog contentWidth="70rem" contentHeight="40rem" title="{i18n&gt;viewBgusa.allProvinceListPopup}" liveChange=".searchAllProvince" confirm=".onConfirmProvince" cancel=".onClosePopupState" items="{/listAllProvince}">
		<columns>
			<Column visible="false">
				<header>
					<Label id="sRowPath" visible="false" />
				</header>
			</Column>
			<Column width="10%">
				<header>
					<Text text="{i18n&gt;viewBgusa.Province}" />
				</header>
			</Column>
			<Column width="35%">
				<header>
					<Text text="{i18n&gt;viewBgusa.ProvinceDescription}" />
				</header>
			</Column>
			<Column width="10%">
				<header>
					<Text text="{i18n&gt;viewBgusa.State}" />
				</header>
			</Column>
			<Column width="35%">
				<header>
					<Text text="{i18n&gt;viewBgusa.StateDescription}" />
				</header>
			</Column>
			<Column width="10%">
				<header>
					<Text text="{i18n&gt;viewBgusa.Country}" />
				</header>
			</Column>
		</columns>
		<ColumnListItem selected="">
			<cells>
				<Text text="" />
				<Text text="{PROVINCE}" />
				<Text text="{PROVINCE_DESCRIPTION}" />
				<Text text="{STATE}" />
				<Text text="{STATE_DESCRIPTION}" />
				<Text text="{COUNTRY_ID}" />
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>